# Access root to add devindec user to sudoers

$ su -

$ apt update

$ apt upgrade

$ apt-get install sudo

$ usermod -a -G sudo devindec

$ reboot now


$ apt install vim

$ apt install openfortivpn

$ vim /etc/openfortivpn/config

# u & u
$ sudo apt-get install wget

# u & u
$ sudo apt install curl

# u & u
$ sudo apt -y install default-jdk

$ java -version
